import java.util.concurrent.*;
import java.util.concurrent.locks.*;

public class DiningPhilosopher {

	
	private static class Phil{
		private Lock left = new ReentrantLock();
		private Lock right = new ReentrantLock();
		
		public Phil(Lock left, Lock right){
			this.left = left;
			this.right = right;
		}
		
		public void ThinkandEat(){
			
		}
	}
}
